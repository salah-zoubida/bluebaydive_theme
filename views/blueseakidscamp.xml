    <div class="activity-details-description">
        <div class="inner-wrapper">
            <div class="tabbed">
                <ul class="tabs clearfix">
                    
                    <li class="tablink" onclick="openPage('itinirary_tab', this)" id="defaultOpen">
                        <a href="#itinerary">Itinerary</a>
                    </li>
                    
                    
                    <li class="tablink" onclick="openPage('iframe_tab', this)" >
                        <a href="#reviews">Reviews</a>
                    </li>
                    
