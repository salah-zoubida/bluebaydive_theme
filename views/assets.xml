			<!--   <link rel="stylesheet" href="/bluebaydive_theme/bxslider-4-4.2.12/src/css/jquery.bxslider.css"/> -->
			  <!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/bxslider/4.2.12/jquery.bxslider.css"/> -->
